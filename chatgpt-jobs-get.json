{
  "openapi": "3.1.0",
  "info": {
    "title": "BizWorx - Get Jobs",
    "description": "View all jobs and projects for Flatline Earthworks business",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://bluecollarbizworx.replit.app"
    }
  ],
  "paths": {
    "/getJobs": {
      "get": {
        "operationId": "getJobs",
        "summary": "Get all jobs",
        "description": "Retrieve all jobs and projects for Flatline Earthworks business",
        "responses": {
          "200": {
            "description": "List of jobs",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          },
                          "title": {
                            "type": "string"
                          },
                          "description": {
                            "type": "string"
                          },
                          "status": {
                            "type": "string"
                          },
                          "clientId": {
                            "type": "integer"
                          },
                          "clientName": {
                            "type": "string"
                          },
                          "startDate": {
                            "type": "string"
                          },
                          "endDate": {
                            "type": "string"
                          },
                          "estimatedCost": {
                            "type": "number"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "API_KEY"
      }
    }
  },
  "security": [
    {
      "BearerAuth": []
    }
  ]
